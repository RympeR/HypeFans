  "scripts": {
    "start-server": "pm2 start ecosystem.config.js",
    "stop-server": "pm2 stop ecosystem.config.js",
    "serve": "serve -s build -l 1919",
    "start": "node scripts/start.js -o",
    "build": "node scripts/build.js --public-url /static/ src/index.html",
    "test": "node scripts/test.js",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{js,css,scss,html}\"",
    "rtl": "webpack",
    "lint:staged": "lint-staged"
