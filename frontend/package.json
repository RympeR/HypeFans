  "lint-staged": {
    "linters": {
      "./src/**/*.js": [
        "prettier --config ./.prettierrc.json  --write",
        "git add"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:staged",
      "pre-push": ""
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
